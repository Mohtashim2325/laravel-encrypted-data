{
    "name": "swisnl/laravel-filesystem-local-encrypted",
    "type": "library",
    "description": "Laravel Local Encrypted Filesystem Driver",
    "keywords": [
        "swisnl",
        "laravel",
        "filesystem",
        "local",
        "encrypted"
    ],
    "homepage": "https://github.com/swisnl/laravel-filesystem-local-encrypted",
    "license": "MIT",
    "authors": [
        {
            "name": "Jasper Zonneveld",
            "email": "jasper@swis.nl",
            "homepage": "https://www.swis.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Swis\\Filesystem\\Encrypted\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swis\\Filesystem\\Encrypted\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
